      "Code": "# Python coding credit to Dynamo Forum user - @Ewan_Opie 2019\r\n# Sastrugi Package https://sites.google.com/view/sastrugi/home\r\n\r\nimport sys\r\nimport clr\r\nclr.AddReference('ProtoGeometry')\r\nfrom Autodesk.DesignScript.Geometry import *\r\n\r\ndef most_common(lst):\r\n    return max(set(lst), key=lst.count)\r\n    \r\n## Remove Duplicates from a List\r\ndef removeDuplicates(mylist):\r\n\tnewlist = list(dict.fromkeys(mylist))\r\n\treturn newlist   \r\n\r\n## Group items in lists by keys\r\ndef group(lists, keys):\r\n\tuniqueKeys = set(keys)\r\n\tresults = []\r\n\tfor uniqueItem in uniqueKeys:\r\n\t    keyGroup = []\r\n\t    for element, key in zip(lists,keys):\r\n\t        if key == uniqueItem:\r\n\t            keyGroup.append(element)\r\n\t   \tresults.append(keyGroup)\r\n\treturn results\t\r\n\r\nclr.AddReference('DSCoreNodes') \r\nimport DSCore\r\nfrom DSCore import *\r\n\r\n## Get the Indexes of Sorted Values\t\r\ndef indexbyvalue(data):\r\n\tsdata = sorted(data)\r\n\tindices = []\r\n\tfor s in sdata:\r\n\t\ti = data.index(s)\r\n\t\tindices.append(i)\r\n\treturn indices\r\n\r\ndef isEdge(line,pts,sample):\r\n\t# Create sample polygon\r\n\tbp = Curve.PointAtParameter(line,0.5)\r\n\tpl = Plane.ByOriginNormal(bp,Vector.ZAxis())\r\n\tci = Circle.ByPlaneRadius(pl,sample/2)\r\n\tpoly = Polygon.RegularPolygon(ci,5)\r\n\t\r\n\t# Count pts that occur within the sample, if greater than 0 then line is an edge\r\n\tcount_pts = 0\r\n\tfor p in pts:\r\n\t\tif Polygon.ContainmentTest(poly,p):\r\n\t\t\tcount_pts += 1\r\n\t\t\t\r\n\tif count_pts > 0:\r\n\t\treturn True\r\n\telse:\r\n\t\treturn False\r\n\t\t\r\npts = IN[0]\r\nrect = IN[1]\r\ncirc = IN[2]\r\naxisL = IN[3]\r\nconf = IN[4]\r\nsideCheck = IN[5]\r\n\r\ntolerance = 10\r\nsample_diameter = 50\r\n\r\nresult = []\r\n\r\nrect_conf = 0\r\ncirc_conf = 0\r\n\r\nif sideCheck == 4:\r\n\t# Workflow for Non Shape\r\n\tfor p in pts:\r\n\t\tif Geometry.DistanceTo(p,rect) < tolerance:\r\n\t\t\trect_conf += 1\r\n\t\tif Geometry.DistanceTo(p,circ) < tolerance:\r\n\t\t\tcirc_conf += 1\r\n\tif rect_conf > circ_conf:\r\n\t\tshape_type = \"Rectangular\"\r\n\t\tOUT = rect,shape_type,rect.Width,rect.Height\r\n\telif circ_conf > rect_conf:\r\n\t\tshape_type = \"Circular\"\r\n\t\tOUT = circ,shape_type,circ.Radius\r\n\r\nelse:\r\n\t#Workfow for Shape\r\n\t\t\r\n\tresult = []\r\n\trect_lines = PolyCurve.Curves(rect)\r\n\trect_count = []\r\n\tedges = []\r\n\tfor r in rect_lines:\r\n\t\tif isEdge(r,pts,sample_diameter):\r\n\t\t\tedges.append(r)\r\n\tif len(edges) == 3:\r\n\t\tshape_type = \"Channel Section\"\r\n\t\tlengths = []\r\n\t\tfor e in edges:\r\n\t\t\tlengths.append(e.Length)\r\n\t\tcol_width = min(lengths)\r\n\t\tcol_length = max(lengths)\r\n\t\t\r\n\telif len(edges) == 2:\r\n\t\tshape_type = \"I Section\"\r\n\t\tmidpts = []\r\n\t\tfor e in edges:\r\n\t\t\tmidpts.append(Curve.PointAtParameter(e,0.5))\r\n\t\tedges.append(Line.ByStartPointEndPoint(midpts[0],midpts[1]))\r\n\t\tcol_length = edges[-1].Length\r\n\t\tcol_width = edges[0].Length\r\n\telse:\r\n\t\tshape_type = \"Unknown Section\"\r\n\t\tcol_width = \"Unknown\"\r\n\t\tcol_length = \"Unknown\"\r\n\t\t\r\n\tOUT = edges, shape_type\t,col_width,col_length\r\n\r\n\r\n",
