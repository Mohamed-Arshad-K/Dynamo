#BTN################
OK = 0x00000000L
ABORTRETRYIGNORE = 0x00000002L
CANCELTRYCONTINUE = 0x00000006L
HELP = 0x00004000L
OKCANCEL = 0x00000001L
RETRYCANCEL = 0x00000005L
YESNO = 0x00000004L
YESNOCANCEL = 0x00000003L

##################
if IN[2] == 0:
	btn = OK
elif IN[2] == 1:
	btn = ABORTRETRYIGNORE
elif IN[2] == 2:
	btn = CANCELTRYCONTINUE
elif IN[2] == 3:
