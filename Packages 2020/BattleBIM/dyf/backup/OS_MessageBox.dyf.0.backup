elif IN[2] == 4:
	btn = OKCANCEL
elif IN[2] == 5:
	btn = RETRYCANCEL
elif IN[2] == 6:
	btn = YESNO
elif IN[2] == 7:
	btn = YESNOCANCEL
else:
	btn = OK

#ICON################
WARNING = 0x00000030L
INFORMATION = 0x00000040L
QUESTION = 0x00000020L
STOP = 0x00000010L
NOICON = 0x40000
##################

if IN[3] == 0:
	icon = WARNING
elif IN[3] == 1:
	icon = INFORMATION
elif IN[3] == 2:
	icon = QUESTION
elif IN[3] == 3:
	icon = STOP
elif IN[3] == 4:
	icon = NOICON
else:
	icon = INFORMATION
##################
def Mbox(title, text, style):
    ctypes.windll.user32.MessageBoxW(0, text, title, style)
#################
testBlock=IN[4]
#OUT=
OUT = Mbox(title, mgs, btn|icon) if testBlock else None

